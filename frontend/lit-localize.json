{
  "$schema": "https://raw.githubusercontent.com/nicolo-ribaudo/nicolo-ribaudo.github.io/main/nicolo-ribaudo.github.io/lit-localize.schema.json",
  "sourceLocale": "en",
  "targetLocales": ["de"],
  "output": {
    "mode": "runtime",
    "outputDir": "src/locales/generated",
    "language": "ts"
  },
  "inputFiles": ["src/**/*.ts"],
  "interchange": {
    "format": "xliff",
    "xliffDir": "src/locales/xliff"
  }
}
